{
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": true
    }
  },
  "extends": "nx/presets/npm.json",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "nxCloudAccessToken": "NWIzOGFmZTItZWY4OC00MmVkLTgwY2YtYTIxZTcwM2Y1ZTA2fHJlYWQtd3JpdGU=",
  "targetDefaults": {
    "@nx/angular:ng-packagr-lite": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"]
    },
    "@nx/rollup:rollup": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"]
    }
  },
  "plugins": [
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ],
  "generators": {
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "buildable": true
    },
    "@nx/react:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "buildable": true
    },
    "@nx/angular:component": {
      "style": "css"
    },
    "@nx/react": {
      "library": {
        "unitTestRunner": "jest"
      }
    }
  }
}
